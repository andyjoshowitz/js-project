<h1>All Instructors</h1>

    <% @instructors.each do |instructor| %>
      <ol>
        <li>
          <%= link_to instructor.full_name, instructor_path(instructor) %>
        </li>
      </ol>
    <% end %>

    <div class="container">
      <div id="instructorsInfo" class="well col-sm-12">
      </div>
    </div>
